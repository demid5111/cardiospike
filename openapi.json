{
  "openapi":"3.0.2",
  "info":{
    "title":"CardioSpike API",
    "version":"0.1.0"
  },
  "paths":{
    "/predict":{
      "post":{
        "summary":"Predict",
        "operationId":"predict_predict_post",
        "requestBody":{
          "content":{
            "application/json":{
              "schema":{
                "$ref":"#/components/schemas/RR"
              }
            }
          },
          "required":true
        },
        "responses":{
          "200":{
            "description":"Successful Response",
            "content":{
              "application/json":{
                "schema":{
                  "$ref":"#/components/schemas/Predictions"
                }
              }
            }
          },
          "500":{
            "description":"Internal Server Error",
            "content":{
              "application/json":{
                "schema":{
                  "$ref":"#/components/schemas/Model500"
                }
              }
            }
          },
          "422":{
            "description":"Validation Error",
            "content":{
              "application/json":{
                "schema":{
                  "$ref":"#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    }
  },
  "components":{
    "schemas":{
      "HTTPValidationError":{
        "title":"HTTPValidationError",
        "type":"object",
        "properties":{
          "detail":{
            "title":"Detail",
            "type":"array",
            "items":{
              "$ref":"#/components/schemas/ValidationError"
            }
          }
        }
      },
      "Model500":{
        "title":"Model500",
        "required":[
          "message"
        ],
        "type":"object",
        "properties":{
          "message":{
            "title":"Message",
            "type":"string"
          }
        }
      },
      "Predictions":{
        "title":"Predictions",
        "required":[
          "study",
          "predictions",
          "errors"
        ],
        "type":"object",
        "properties":{
          "study":{
            "title":"study",
            "type":"string",
            "description":"unique rr study identifier",
            "example":"user_id_0"
          },
          "predictions":{
            "title":"predictions",
            "type":"array",
            "items":{
              "type":"integer"
            },
            "description":"list of anomaly predictions (0 or 1)",
            "example":[
              0,
              0,
              1,
              1,
              1,
              0
            ]
          },
          "errors":{
            "title":"errors",
            "type":"array",
            "items":{
              "type":"integer"
            },
            "description":"list errors (0 or 1)",
            "example":[
              1,
              0,
              0,
              0,
              0,
              0
            ]
          }
        }
      },
      "RR":{
        "title":"RR",
        "required":[
          "study",
          "sequence"
        ],
        "type":"object",
        "properties":{
          "study":{
            "title":"study",
            "type":"string",
            "description":"unique rr study identifier",
            "example":"user_id_0"
          },
          "sequence":{
            "title":"sequence",
            "type":"array",
            "items":{
              "type":"integer"
            },
            "description":"RR intervals in milliseconds",
            "example":[
              200,
              300,
              200,
              300,
              200,
              300
            ]
          }
        }
      },
      "ValidationError":{
        "title":"ValidationError",
        "required":[
          "loc",
          "msg",
          "type"
        ],
        "type":"object",
        "properties":{
          "loc":{
            "title":"Location",
            "type":"array",
            "items":{
              "type":"string"
            }
          },
          "msg":{
            "title":"Message",
            "type":"string"
          },
          "type":{
            "title":"Error Type",
            "type":"string"
          }
        }
      }
    }
  }
}
